<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.mu"
		name="Palm OS (Mu)"
		version="1.3.0.8"
		provider-name="guicrith / meepingsnesroms">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>prc|pqa|img</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>true</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">

Supported files: .prc, .pqa, .img</description>
		<license>Non-commercial</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.mu</source>
		<summary lang="en_GB">Palm OS (Mu) v1.3.0</summary>
		<description lang="en_GB">Mu is a new Palm OS emulator targeting compatibility, speed and accuracy in that order.

Supported files: .prc, .pqa, .img</description>
	</extension>
</addon>
